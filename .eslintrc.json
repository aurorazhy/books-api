{
   "env": {
      "browser": true,
      "es2021": true,
      "node": true
   },
   "extends": "airbnb-base",
   "overrides": [],
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "rules": {
      "linebreak-style": 0,
      "eol-last": 0,
      "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
      "indent": ["error", 3],
      "no-console": "off",
      "no-unused-vars": "off",
      "import/extensions": "off",
      "implicit-arrow-linebreak": ["error", "below"]
   }
}
